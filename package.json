{
  "name": "12-column-grid",
  "version": "1.3.4",
  "description": "Provides CSS classes for creating 12 column grid layout",
  "browser": "./dist/grid.min.css",
  "scripts": {
    "watch": "node-sass --watch src/grid.scss > dist/grid.css --output-style expanded",
    "build:minify": "node-sass src/grid.scss > dist/grid.min.css --output-style compressed",
    "build:expanded": "node-sass src/grid.scss > dist/grid.css --output-style expanded",
    "build": "npm run build:expanded && npm run build:minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KumarSidharth/12-column-grid.git"
  },
  "keywords": [
    "12-column-grid-layout",
    "12-column-grid",
    "grid.css",
    "grid-css",
    "css",
    "grids.css",
    "css-grid",
    "scss",
    "grid.scss",
    "grid-scss",
    "scss-grid",
    "grids.scss",
    "grids-scss",
    "scss-grids",
    "grid",
    "grid.sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "author": "Kumar Sidharth <k.sidharth1994@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KumarSidharth/12-column-grid/issues"
  },
  "homepage": "https://github.com/KumarSidharth/12-column-grid#readme",
  "devDependencies": {
    "node-sass": "^4.14.1"
  }
}
